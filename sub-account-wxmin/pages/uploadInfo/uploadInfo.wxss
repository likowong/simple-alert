/* pages/uploadInfo/uploadInfo.wxss */
.main{
    padding: 40rpx 0 0 40rpx;
}
.tag{
    position: relative;
    padding-left: 20rpx;
    margin-bottom: 40rpx;
    font-weight: 600;
}
.tag:before{
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    width: 6rpx;
    height: 24rpx;
    transform: translateY(-50%);
    background-color: #00a0e8;
}
.content-list{
    border-bottom: 1px solid #e7ebee;
}
.content-list-two{
    margin-top: 56rpx;
}
.img-big-box{
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: flex-start;
    align-content: flex-start;
    margin-bottom: 30rpx;
}
.img-box{
    position: relative;
    margin: 0 10rpx 10rpx 0;
    width: 160rpx;
    height: 160rpx;
    border: 1px solid #e7ebee;
}
.img-box image {
    width: 160rpx;
    height: 160rpx;
}